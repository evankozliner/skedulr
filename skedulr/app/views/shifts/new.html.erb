<h1>Create new shift</h1>
<%= form_for @shift do |f| %>
  <div class="col-md-6">
    <%= render partial: "date", locals: {name: :start, f: f} %>
  </div>
  <div class="col-md-6">
    <%= render partial: "date", locals: {name: :stop, f: f} %>
  </div>
  <%= f.submit class: 'btn btn-success'%>
<% end %>
